<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="persistencia">
  <class dynamic-insert="true"
         dynamic-update="true" name="Dueño" table="dueño">
   <id name="idDueño"
            column="idDueño"
            type="java.lang.Integer"
            unsaved-value="null" >
            <generator class="native">
            </generator>
        </id>
     <property name="Nombre" column="Nombre"/>
     <property name="Direccion" column="Direccion" />
     <property name="Telefono" column="Telefono" />

    <many-to-one name="Dueño" class="Dueño" fetch="select">
            <column name="IdDueño" not-null="true" />
    </many-to-one>
  </class>
</hibernate-mapping>
